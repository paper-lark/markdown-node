{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "plugins": ["prettier", "flowtype", "node"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "no-trailing-whitespace": true,
    "semicolon": true,
    "no-console": false,
    "no-empty": false
  }
}
